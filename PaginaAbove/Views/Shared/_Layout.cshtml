
<!DOCTYPE html>
<html>
<head>
    <title>miPortofolio</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Onepage Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
		Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href="~/css/bootstrap.css" rel='stylesheet' type='text/css' />
    <link href="~/css/style.css" rel="stylesheet" type="text/css" media="all" />

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/js/move-top.js"></script>
    <script type="text/javascript" src="~/js/easing.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function($) {
            $(".scroll").click(function(event){
                event.preventDefault();
                $('html,body').animate({scrollTop:$(this.hash).offset().top},1200);
            });
        });
    </script>
    <!--Font miPortfolio-->
    <link href="https://fonts.googleapis.com/css2?family=Noto+Serif+Display:ital,wght@1,300&display=swap" rel="stylesheet">
    <style>
        .logo {
            font-family: 'Noto Serif Display', serif;
            font-size: 30px;
            color: #BEB6B4;
        }
    </style>
</head>

<body>
    <div class="header" id="head" style="background: #2e3e4e;">
        <div class="container">
            <div class="header-top">
                <div>
                    <h4 class="logo">miPortfolio</h4>
                </div>
                <div class="top-menu">
                    <span class="menu"> </span>
                    <nav class="cl-effect-5">
                        <ul>
                            <li><a class="active" href=""><span data-hover="Inicio">Inicio</span></a></li>
                            <li><a href="#about" class="scroll"><span style="padding: 0px 10px 0px 0px" data-hover="Sobre mí">Sobre mí</span></a></li>
                            <li><a href="#work" class="scroll"><span data-hover="Proyectos">Proyectos</span></a></li>
                            <li><a href="#habilidades" class="scroll"><span style="padding: 0px 10px 0px 0px" data-hover="Habilidades">Habilidades</span></a></li>
                            <li><a href="#contact" class="scroll"><span data-hover="contacto">contacto</span></a></li>
                        </ul>
                    </nav>
                </div>
                <!--script-nav-->
                <script>
                    $("span.menu").click(function(){
                        $(".top-menu ul").slideToggle("slow" , function(){});
                    });
                </script>
                <div class="clearfix"></div>
            </div>
            <div class="index-banner">
                <div class="wmuSlider example1">
                    <div class="wmuSliderWrapper">
                        <article style="position: absolute; width: 100%; opacity: 0;">
                            <div class="banner-wrap">
                                <div class="banner_center">
                                    <h1>Hola,</h1>
                                    <h2>te doy la bienvenida a<span> mi portfolio</span></h2>
                                    @*<h2>from morocco</h2>*@
                                </div>
                            </div>
                        </article>
                        <article style="position: relative; width: 100%; opacity: 1;">
                            <div class="banner-wrap">
                                <div class="banner_center">
                                    <h1>Te invito</h1>
                                    <h2>a conocer <span>mis proyectos</span></h2>
                                </div>
                            </div>
                        </article>
                        <article style="position: absolute; width: 100%; opacity: 0;">
                            <div class="banner-wrap">
                                <div class="banner_center">
                                    <h1>y tambien</h1>
                                    <h2>un poco<span> acerca de mí.</span></h2>
                                    <h2>Que lo disfrutes.</h2>
                                </div>
                            </div>
                        </article>
                    </div>
                </div>
                <script src="~/js/jquery.wmuSlider.js"></script>
                <script>
                    $('.example1').wmuSlider();
                </script>
            </div>
        </div>
    </div>

    <div class="content">
        @RenderBody()
    </div>


    <div class="footer-section" id="contact">
        <div class="container">
            <div class="contact-header">
                <h3> contacto</h3>
            </div>
            <div class="social-icon">
                <a href="#"><i class="icon1"></i></a>
                <a href="#"><i class="icon2"></i></a>
                <a href="#"><i class="icon3"></i></a>
                <a href="#"><i class="icon4"></i></a>
                <a href="#"><i class="icon5"></i></a>
                <a href="#"><i class="icon6"></i></a>
                <a href="#"><i class="icon7"></i></a>
                <a href="#"><i class="icon8"></i></a>
            </div>
            <div class="contact">
                <form method="POST" action="EnvioMail">
                    <div class="col-md-4 contactgrid">
                        <input style="background-color: #2e3e4e;" name="nombre" type="text" class="text" value="Nombre" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = ' Nombre';}">
                    </div>
                    <div class="col-md-4 contactgrid">
                        <input name="email" type="text" class="text" value="Email" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Email';}">
                    </div>
                    <div class="col-md-4 contactgrid">
                        <input name="celular" type="text" class="text" value="Celular" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Celular';}">
                    </div>
                    <div class="col-md-8 contactgrid1">
                        <textarea name="mensaje" onfocus="if(this.value == 'Mensaje') this.value='';" onblur="if(this.value == '') this.value='Mensaje';">Mensaje</textarea>
                    </div>
                    <div class="col-md-4 contactgrid2">
                        <input type="submit" value="[Enviar Mensaje]">
                    </div>
                    <div class="clearfix"></div>
                </form>
                @if (TempData["ok"] != null)
                {
                    <script type="text/javascript">
                        window.onload = function () {
                            alert("Tus datos fueron enviados correctamente");                            
                        };
                    </script>
                }
                <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
            </div>
            <div class="footer-bottom">
                <p>&copy; fedeetorancio@gmail.com | <a href="" target="target_blank">Aviso Legal</a> | <a href="" target="target_blank">Politicas de privacidad</a> | <a href="" target="target_blank">Todos los derechos reservados</a></p>
            </div>
            <script type="text/javascript">
                $(document).ready(function() {
                    /*
                    var defaults = {
                    containerID: 'toTop', // fading element id
                    containerHoverID: 'toTopHover', // fading element hover id
                    scrollSpeed: 1200,
                    easingType: 'linear'
                    };
                    */
                    $().UItoTop({ easingType: 'easeOutQuart' });
                });
            </script>
            <a href="#" id="toTop" style="display: block;"> <span id="toTopHover" style="opacity: 1;"> </span></a>
        </div>
    </div>

</body>
</html>





